#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
    int a,b,c,max;
    cout<<"enter the three numbers: ";
    cin>>a>> b>> c;
    if(a>b&&a>c)
    {
        cout<<"the greatest of the three numbers is: "<<a;
    }
    else if(b>a&&b>c)
    {
        cout<<"the greatest of three numbers is: "<<b;
    }
    else
    {
        cout<<"the greatest of three numbers is: "<<c;
    }
    getch();
}
